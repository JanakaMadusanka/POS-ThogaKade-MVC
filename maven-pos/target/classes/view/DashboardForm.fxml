<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="dashboardFormPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DashboardFormController">
   <children>
      <Button fx:id="btnCustomer" layoutX="56.0" layoutY="108.0" mnemonicParsing="false" onAction="#btnCustomerOnAction" prefHeight="58.0" prefWidth="479.0" text="Customer">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button fx:id="btnItem" layoutX="56.0" layoutY="183.0" mnemonicParsing="false" onAction="#btnItemOnAction" prefHeight="52.0" prefWidth="479.0" text="Item">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button fx:id="btnOrderDetail" layoutX="56.0" layoutY="316.0" mnemonicParsing="false" onAction="#btnOrderDetailOnAction" prefHeight="52.0" prefWidth="479.0" text="Order Detail">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button fx:id="btnOrders" layoutX="56.0" layoutY="249.0" mnemonicParsing="false" onAction="#btnOrdersOnAction" prefHeight="52.0" prefWidth="479.0" text="Orders">
